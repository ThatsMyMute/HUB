-- Scripts:

local function BGOPLH_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		local player = game:GetService("Players").LocalPlayer
	
	
		local gun = player.Backpack:FindFirstChild("Remington 870")
	
	
		local user = require(gun:FindFirstChild("GunStates"))
	
		user["Damage"] = 999
	
		user["MaxAmmo"] = 9999991
	
		user["StoredAmmo"] = 9999991
	
		user["FireRate"] = 0.05
	
		user["AmmoPerClip"] = 9999991
	
		user["Range"] = 500
	
		user["ReloadTime"] = 0.05
	
		user["Bullets"] = 600
	
		user["AutoFire"] = true 
	end)
end
coroutine.wrap(BGOPLH_fake_script)()
