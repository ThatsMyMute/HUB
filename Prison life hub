		followed = false
		traill = false
		noclip = false
		annoying = false
		hwalk = false
		droppinghats = false
		droppingtools = false
		flying = false
		spamdelay = 1
		spamming = false
		spammingpm = false
		cbringing = false
		remotes = true
		added = true
		binds = false
		stopsitting = false
		clickgoto = false
		gettingtools = false
	
